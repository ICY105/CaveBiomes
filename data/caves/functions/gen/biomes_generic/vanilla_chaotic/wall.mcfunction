
execute if score $gen.out_0 cave_data matches 000..199 run setblock ~ ~ ~ minecraft:stone
execute if score $gen.out_0 cave_data matches 200..399 run setblock ~ ~ ~ minecraft:cobblestone
execute if score $gen.out_0 cave_data matches 500..699 run setblock ~ ~ ~ minecraft:stone_bricks
execute if score $gen.out_0 cave_data matches 700..799 run setblock ~ ~ ~ minecraft:granite
execute if score $gen.out_0 cave_data matches 800..899 run setblock ~ ~ ~ minecraft:andesite
execute if score $gen.out_0 cave_data matches 900..999 run setblock ~ ~ ~ minecraft:diorite
